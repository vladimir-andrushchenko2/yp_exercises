{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: g++-11 build project",
			"command": "/usr/local/bin/g++-11",
			"args": [
				"-std=c++17",
				"-g",
				"-Wall",
				"../Ini/main.cpp",
				"../Ini/ini.cpp",
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /usr/local/bin/g++-11"
		}
	]
}